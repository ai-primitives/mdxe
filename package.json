{
  "name": "mdxe",
  "version": "1.1.2",
  "description": "Zero-Config Executable MDX",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    }
  },
  "bin": {
    "mdxe": "./bin/cli.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --ignore-pattern \"src/integration/**\" --ignore-pattern \"**/.next/**\" --ignore-pattern \"**/node_modules/**\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" \"*.{md,mdx}\"",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "typescript",
    "template",
    "package",
    "cli"
  ],
  "author": "AI Primitives",
  "license": "MIT",
  "homepage": "https://mdx.org.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ai-primitives/mdxe.git"
  },
  "bugs": {
    "url": "https://github.com/ai-primitives/mdxe/issues"
  },
  "engines": {
    "node": ">=20.18.1"
  },
  "dependencies": {
    "@mdx-js/mdx": "3.1.0",
    "@mdx-js/react": "3.1.0",
    "ai-functions": "^0.3.0",
    "gray-matter": "4.0.3",
    "mdxld": "1.0.1",
    "next-mdxld": "1.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark-gfm": "4.0.0",
    "remark-mdxld": "0.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@mdx-js/loader": "^3.1.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/debug": "^4.1.12",
    "@types/mdast": "^4.0.4",
    "@types/mdx-js__react": "^1.5.8",
    "next": "^14.0.0",
    "@types/node": "^22.10.2",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "18.2.0",
    "@types/unist": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "autoprefixer": "^10.4.20",
    "cosmiconfig": "^9.0.0",
    "debug": "^4.4.0",
    "eslint": "^9.17.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "remark-frontmatter": "^5.0.0",
    "semantic-release": "^24.2.0",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.7.0",
    "typescript": "^5.7.2",
    "vfile": "^6.0.3",
    "vitest": "^2.1.8",
    "webpack": "^5.97.1"
  }
}
